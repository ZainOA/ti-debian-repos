#!/usr/bin/make -f

srctree ?= .

build-indep:
build-arch:
	$(MAKE) -j 16 KERNELRELEASE=6.1.46+940fab2b81d2867133444eed01ba1b20c294965d    ARCH=arm64 	KBUILD_BUILD_VERSION=1 -f $(srctree)/Makefile am62x_var_defconfig
	$(MAKE) -j 16 KERNELRELEASE=6.1.46+940fab2b81d2867133444eed01ba1b20c294965d    ARCH=arm64 	KBUILD_BUILD_VERSION=1 -f $(srctree)/Makefile

build: build-arch

binary-indep:
binary-arch: build-arch
	$(MAKE) -j 16 KERNELRELEASE=6.1.46+940fab2b81d2867133444eed01ba1b20c294965d    ARCH=arm64 	KBUILD_BUILD_VERSION=1 -f $(srctree)/Makefile intdeb-pkg

clean:
	rm -rf debian/files debian/linux-*
	$(MAKE) clean

binary: binary-arch
