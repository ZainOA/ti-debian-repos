#!/usr/bin/make -f

srctree ?= .

build-indep:
build-arch:
	$(MAKE) -j 16 KERNELRELEASE=6.1.83-k3-var    ARCH=arm64 	KBUILD_BUILD_VERSION=1 LOCALVERSION=+ca705eb7c9d102423b529319e8192634cc7bfbbd -f $(srctree)/Makefile am62x_var_defconfig CONFIG_LOCALVERSION_AUTO=y
	$(MAKE) -j 16 KERNELRELEASE=6.1.83-k3-var    ARCH=arm64 	KBUILD_BUILD_VERSION=1 LOCALVERSION=+ca705eb7c9d102423b529319e8192634cc7bfbbd -f $(srctree)/Makefile

build: build-arch

binary-indep:
binary-arch: build-arch
	$(MAKE) -j 16 KERNELRELEASE=6.1.83-k3-var    ARCH=arm64 	KBUILD_BUILD_VERSION=1 LOCALVERSION=+ca705eb7c9d102423b529319e8192634cc7bfbbd -f $(srctree)/Makefile intdeb-pkg

clean:
	rm -rf debian/files debian/linux-*
	$(MAKE) clean

binary: binary-arch
